    "jsx-loader": "^0.12.2",
    "jsxhint": "^0.8.1",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-mocha": "^0.1.10",
    "karma-sinon-chai": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.2",
    "karma-webpack": "^1.3.1",
